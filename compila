rm -rf bin/*
nasm -f aout src/libasm.asm -o bin/libasm.o
nasm -f aout src/kstart.asm -o bin/kstart.o
nasm -f aout src/loader.asm -o bin/kstart.o
gcc -c src/kernel.c -o bin/kernel.o -fno-builtin
gcc -c src/libc.c  -o bin/libc.o -fno-builtin
ld -T link.ld -o bin/kernel.bin bin/kernel.o bin/kstart.o bin/libc.o bin/libasm.o
